---
interface Props {
    title: string;
    subtitle: string;
}

const { title, subtitle } = Astro.props as Props;
---

<div class="flex flex-col items-center pt-10">
    <h1 class="text-5xl font-mono text-pure-green mb-4">{title}</h1>
    <h2 class="text-xl font-mono text-green-400">{subtitle}</h2>
</div>
